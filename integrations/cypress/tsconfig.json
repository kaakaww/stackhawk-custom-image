{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Cypress",
    "compilerOptions": {
        "baseUrl": ".",
        "target": "es6",
        "module": "esnext",
        "moduleResolution": "node",
        "lib": [
          "es2015",
          "dom"
        ],
        "rootDir": "./",
        "outDir": "dist",
        "declaration": true,
        "strict": true,
        "types": [
          "cypress"
        ],
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "resolveJsonModule": false
      },
      "include": ["e2e/**/*.ts", "support/**/*.ts"]
}